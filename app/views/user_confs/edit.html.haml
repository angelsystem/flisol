.block
  .secondary-navigation
    %ul.wat-cf
      %li.first= link_to "#{t("web-app-theme.list", :default => "List")}", user_confs_path
      %li= link_to "#{t("web-app-theme.new", :default => "New")}", new_user_conf_path
      %li.active= link_to "#{t("web-app-theme.edit", :default => "Edit")}", edit_user_conf_path
  .content
    %h2.title
      = t("web-app-theme.edit", :default => "Edit")
      UserConf
    .inner
      = form_for @user_conf, :url => user_conf_path(@user_conf), :html => { :class => :form } do |f|
        = render :partial => "form", :locals => {:f => f}
- content_for :sidebar, render(:partial => 'sidebar')
